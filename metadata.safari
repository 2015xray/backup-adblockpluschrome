[default]
inherit = metadata.common

[general]
basename = adblockplussafari
version = 0.1
author = Eyeo GmbH
homepage = http://adblockplus.org/

[mapping]
ext/common.js = safari/common.js
ext/background.js = safari/background.js
ext/content.js = safari/content.js
Icon.png = icons/abp-128.png

[convert_js]
lib/io.js = lib/websql/io.js
  --arg module=true

[popovers]
popup_filename = popup.html
popup_width = 250
popup_height = 420
#popup_width = 155
#popup_height = 70

[toolbar_items]
item_image = icons/abp-16.png
item_popover = popup
item_menu = menu
item_label = will_be_replaced_with_translation
item_tool_tip = will_be_replaced_with_translation

[convert_img]
