{
	"name": "Adblock Plus",
	"version": "1.0.20",
	"minimum_chrome_version": "6.0",
	"description": "__MSG_description__",
	"default_locale": "en",
	"page_action": {
		"default icon": "icons/abp-16.png",
		"default title": "Adblock Plus",
		"popup": "popup.html"
	},
	"icons": {
		"16": "icons/abp-16.png",
		"32": "icons/abp-32.png",
		"48": "icons/abp-48.png",
		"128": "icons/abp-128.png"
	},
	"permissions": [
		"tabs", "http://*/*", "https://*/*", "contextMenus"
	],
	"background_page": "background.html",
	"options_page": "options.html",
	"content_scripts": [
	{
 		"matches": ["http://*/*", "https://*/*"],
		"js": ["initial-block.js"],
		"run_at": "document_start",
		"all_frames": true
	},
 	{
 		"matches": ["http://*/*", "https://*/*"],
 		"js": ["blocker.js", "disabletextads.js"],
 		"run_at": "document_end",
 		"all_frames": true
 	}
	]
}
