{
  "name": "__MSG_name__",
  "version": "1.2",
  "minimum_chrome_version": "16.0",
  "description": "__MSG_description__",
  "default_locale": "en",
  "update_url": "https://adblockplus.org/devbuilds/adblockpluschrome/updates.xml",
  "page_action": {
    "default icon": "icons/abp-19.png",
    "default title": "Adblock Plus",
    "popup": "popup.html"
  },
  "icons": {
    "16": "icons/abp-16.png",
    "32": "icons/abp-32.png",
    "48": "icons/abp-48.png",
    "128": "icons/abp-128.png"
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*", "contextMenus", "webRequest", "webRequestBlocking", "unlimitedStorage", "experimental"
  ],
  "background_page": "background.html",
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["http://*.youtube.com/*", "https://*.youtube.com/*", "http://*.youtube-nocookie.com/*", "https://*.youtube-nocookie.com/*"],
      "js": ["include.experimental.js", "include.youtube.js"],
      "run_at": "document_start",
      "all_frames": true
    },
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["modules/patches.js", "modules/FilterClasses.jsm", "modules/Matcher.jsm", "include.experimental.js", "include.preload.js"],
      "run_at": "document_start",
      "all_frames": true
    },
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["include.postload.js", "include.textads.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}
